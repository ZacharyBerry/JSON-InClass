<Window x:Class="GameOfThronesJSON.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GameOfThronesJSON"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="https://ewedit.files.wordpress.com/2017/06/snow_got.jpg?w=2000" Stretch="UniformToFill"/>
        </Grid.Background>
        <TextBlock x:Name="quote" HorizontalAlignment="Left" Margin="659,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="134" Width="112"/>
    </Grid>
</Window>
